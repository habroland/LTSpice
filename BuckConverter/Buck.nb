Cell[BoxData[{
 RowBox[{
  RowBox[{"Use", " ", "Ansatz", " ", 
   RowBox[{"il", "[", "t", "]"}]}], "=", 
  RowBox[{"i0", " ", "+", " ", 
   RowBox[{"m", " ", "t"}], " ", "+", " ", 
   RowBox[{"q", " ", 
    SuperscriptBox["t", "2"]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Continuity", " ", "is", " ", "already", " ", "built"}], "-", 
  RowBox[{
  "in", " ", "in", " ", "this", " ", "formula"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["il", 
    RowBox[{"t", "<", "ta"}]], "[", "t", "]"}], " ", "=", " ", 
  RowBox[{"i0", " ", "+", " ", 
   RowBox[{"ma", " ", "t"}], " ", "+", " ", 
   RowBox[{"qa", " ", 
    SuperscriptBox["t", "2"]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["il", 
    RowBox[{"t", "<", "tc"}]], "[", "t", "]"}], " ", "=", " ", 
  RowBox[{
   RowBox[{
    SubscriptBox["il", 
     RowBox[{"t", "<", "ta"}]], "[", "ta", "]"}], " ", "+", " ", 
   RowBox[{"mb", " ", 
    RowBox[{"(", 
     RowBox[{"t", "-", "ta"}], ")"}]}], " ", "+", " ", 
   RowBox[{"qb", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"t", "-", "ta"}], ")"}], "2"]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"We", " ", "have", " ", "5", " ", 
   RowBox[{"variables", ":", " ", "i0"}]}], ",", " ", "ma", ",", " ", "mb", 
  ",", " ", 
  RowBox[{"qa", " ", "and", " ", 
   RowBox[{"qb", ".", " ", "We"}], " ", "will", " ", "see", " ", "that", " ", 
   "we", " ", "have", " ", "5", " ", "equations", " ", "to", " ", "solve", 
   " ", "for", " ", "them"}]}]}], "Text",
