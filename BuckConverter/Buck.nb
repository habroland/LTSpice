Cell["\<\
First, the quadratic coefficients are determined by substituting each part of \
the piecewise function into the differential equation for il[t] and comparing \
the 0th-order terms.\
\>", "Text",
 CellChangeTimes->{{3.818255036099845*^9, 
  3.818255145159977*^9}},ExpressionUUID->"faf85a3e-a568-4b16-8bc5-\
437362b7072e"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"QuadTerms", "=", 
  RowBox[{
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"V", "/", "R"}], " ", "\[Equal]", 
        RowBox[{"i0", " ", "+", " ", 
         RowBox[{"L", " ", 
          RowBox[{"ma", "/", "R"}]}], "+", 
         RowBox[{"L", " ", "CC", " ", "2", " ", "qa"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"0", "\[Equal]", 
        RowBox[{"i0", "+", 
         RowBox[{"ma", " ", "ta"}], "-", 
         RowBox[{"mb", " ", "ta"}], "+", 
         RowBox[{"qa", " ", 
          SuperscriptBox["ta", "2"]}], "+", 
         RowBox[{"qb", " ", 
          SuperscriptBox["ta", "2"]}], "+", 
         RowBox[{
          FractionBox["L", "R"], 
          RowBox[{"(", 
           RowBox[{"mb", "-", 
            RowBox[{"2", "qb", " ", "ta"}]}], ")"}]}], "+", 
         RowBox[{"L", " ", "CC", " ", "2", "qb"}]}]}]}], 
      "\[IndentingNewLine]", "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"qa", ",", "qb"}], "}"}]}], "]"}], "//", 
   "Simplify"}]}]], "Input",
 CellChangeTimes->{{3.818022289310956*^9, 3.8180224009600005`*^9}, {
   3.8180224574600677`*^9, 3.8180224605674343`*^9}, {3.8180225187199683`*^9, 
   3.8180225206606493`*^9}, 3.8181220562267327`*^9, {3.818122668192439*^9, 
   3.8181226963324423`*^9}, {3.818122730852187*^9, 3.8181227648321133`*^9}, {
   3.8181231182125998`*^9, 3.8181231393828745`*^9}, 3.8181672743248396`*^9, {
   3.818167357400015*^9, 3.818167359125128*^9}, {3.818255982280401*^9, 
   3.8182559882405853`*^9}},ExpressionUUID->"dd03fb5e-66f7-4df9-8b46-\
71ca91860419"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"qa", "\[Rule]", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{
        RowBox[{"L", " ", "ma"}], "+", 
        RowBox[{"i0", " ", "R"}], "-", "V"}], 
       RowBox[{"2", " ", "CC", " ", "L", " ", "R"}]]}]}], ",", 
    RowBox[{"qb", "\[Rule]", 
     FractionBox[
      RowBox[{
       RowBox[{
        RowBox[{"-", "2"}], " ", "CC", " ", "L", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"L", " ", "mb"}], "+", 
          RowBox[{"R", " ", 
